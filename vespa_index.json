---
layout: empty
index: false
---
[
{% for page in site.pages %}
{% unless page.index == false %}
{
    "data" : "{{ page.index }}",
    "put": "id:doc:doc::{{ site.search.doctype }}{{ page.url | relative_url }}",
    "fields": {
        "doctype": "{{ site.search.doctype }}",
        "path": "{{ site.github.url }}{{ page.url | relative_url }}",
        "title": {{ page.title | strip_html | normalize_whitespace | jsonify }},
        "content": {{ page.content | strip_html | normalize_whitespace | jsonify }}
    }
}{% unless forloop.last %},{% endunless %}
{% endunless %}
{% endfor %}
];
